{
  "name": "@mcereal/nestjsdb2",
  "version": "0.0.1",
  "description": "NestJS module for interacting with IBM DB2 databases",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "install-driver": "ts-node src/cli/db2-cli.ts",
    "test": "jest --detectOpenHandles --forceExit",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "start:dev": "nest start --watch",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "prepublishOnly": "npm run lint && npm run test && npm run build",
    "prepare": "npm run build",
    "clean": "rimraf dist",
    "postinstall": "node dist/install/install-driver.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mcereal/nestjsdb2.git"
  },
  "keywords": [
    "nestjs",
    "ibm",
    "db2",
    "database",
    "decorators",
    "typescript",
    "transaction",
    "query-builder"
  ],
  "author": "Michael Curtis <mpbcurtis@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mcereal/nestjsDb/issues"
  },
  "homepage": "https://github.com/mcereal/nestjsdb2#readme",
  "dependencies": {
    "@nestjs/cache-manager": "^2.2.2",
    "@nestjs/common": "^10.4.1",
    "@nestjs/config": "^3.2.3",
    "@nestjs/core": "^10.4.1",
    "@nestjs/platform-express": "^10.4.3",
    "@nestjs/terminus": "^10.2.3",
    "adm-zip": "^0.5.15",
    "axios": "^1.7.5",
    "cache-manager": "^5.7.6",
    "cache-manager-redis-yet": "^5.1.4",
    "class-validator": "^0.14.1",
    "dotenv": "^16.4.5",
    "fs-extra": "^11.2.0",
    "generic-pool": "^3.9.0",
    "ibm_db": "^3.2.4",
    "jsonwebtoken": "^9.0.2",
    "kerberos": "^2.2.0",
    "ldapjs": "^3.0.7",
    "reflect-metadata": "^0.1.13"
  },
  "devDependencies": {
    "@nestjs/testing": "^10.4.1",
    "@types/cache-manager": "^4.0.6",
    "@types/dotenv": "^6.1.1",
    "@types/jest": "^29.5.12",
    "@types/node": "^22.5.0",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.0.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typedoc": "^0.26.6",
    "typescript": "^5.5.4"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "engines": {
    "node": ">=14.0.0",
    "npm": ">=7.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
