{
  "name": "@mcereal/nestjsdb2",
  "version": "1.1.0",
  "description": "NestJS module for interacting with IBM DB2 databases",
  "author": "Michael Curtis <mpbcurtis@gmail.com>",
  "readme": "README.md",
  "contributors": [
    {
      "name": "Michael Curtis",
      "email": "mpbcurtis@gmail.com",
      "url": "https://github.com/mcereal"
    }
  ],
  "maintainers": [
    {
      "name": "Michael Curtis",
      "email": "mpbcurtis@gmail.com",
      "url": "https://github.com/mcereal"
    }
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "commonjs",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mcereal/nestjsdb2/issues"
  },
  "homepage": "https://github.com/mcereal/nestjsdb2#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mcereal/nestjsdb2.git"
  },
  "keywords": [
    "nestjs",
    "ibm",
    "db2",
    "database",
    "decorators",
    "typescript",
    "transactions",
    "query-builder",
    "sql",
    "migrations",
    "cache",
    "health-check",
    "security"
  ],
  "scripts": {
    "build": "tsc -b tsconfig.build.json",
    "build:dev": "cd testconnection && npm run build",
    "test": "jest --detectOpenHandles --forceExit --config jest.config.ts",
    "test:watch": "jest --watch --config jest.config.ts",
    "test:coverage": "jest --detectOpenHandles --forceExit --config jest.config.ts --coverage",
    "start:dev": "cd testconnection && nest start --watch",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "prepublishOnly": "npm run lint && npm run test && npm run build",
    "prepare": "npm run build",
    "clean": "rimraf dist",
    "doc": "typedoc",
    "postinstall": "node scripts/check-rossetta.mjs"
  },
  "dependencies": {
    "@nestjs/cache-manager": "^2.2.2",
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^3.2.3",
    "cache-manager": "^5.7.6",
    "cache-manager-redis-yet": "^5.1.4",
    "class-validator": "^0.14.1",
    "generic-pool": "^3.9.0",
    "ibm_db": "^3.2.4",
    "jsonwebtoken": "^9.0.2",
    "kerberos": "^2.2.0",
    "keyv": "^5.0.3",
    "ldapjs": "^3.0.7",
    "reflect-metadata": "^0.2.2"
  },
  "peerDependencies": {
    "@nestjs/common": "^10.0.0",
    "typescript": ">=4.0.0"
  },
  "devDependencies": {
    "@nestjs/testing": "^10.4.1",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^10.3.4",
    "@semantic-release/npm": "^12.0.1",
    "@types/cache-manager": "^4.0.6",
    "@types/jest": "^29.5.12",
    "@types/node": "^22.5.0",
    "@typescript-eslint/eslint-plugin": "^8.6.0",
    "@typescript-eslint/parser": "^8.6.0",
    "eslint": "^9.11.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "jest": "^29.7.0",
    "prettier": "^3.0.0",
    "rimraf": "^6.0.1",
    "semantic-release": "^24.1.1",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typedoc": "^0.26.6",
    "typescript": "^5.5.4"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE",
    "CHANGELOG.md",
    "package.json"
  ],
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=10.0.0"
  },
  "os": [
    "darwin",
    "linux",
    "win32"
  ],
  "publishConfig": {
    "access": "public"
  },
  "funding": {
    "type": "individual",
    "url": "https://github.com/sponsors/mcereal"
  },
  "sideEffects": false,
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    }
  }
}
