{
  "name": "ibm_db2",
  "version": "0.0.1",
  "description": "NestJS module for IBM DB2",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "start:dev": "ts-node src/index.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "prepublishOnly": "npm run build && npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mcereal/ibm_db2.git"
  },
  "keywords": [
    "nestjs",
    "ibm",
    "db2",
    "database",
    "decorators",
    "typescript",
    "cache",
    "retry",
    "transaction"
  ],
  "author": "Michael Curtis <mpbcurtis@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mcereal/ibm_db2/issues"
  },
  "homepage": "https://github.com/mcereal/ibm_db2#readme",
  "dependencies": {
    "@nestjs/common": "^10.4.1",
    "@nestjs/core": "^10.4.1",
    "@nestjs/terminus": "^10.2.3",
    "cache-manager": "^5.7.6",
    "ibm_db": "^3.2.4",
    "reflect-metadata": "^0.1.13"
  },
  "devDependencies": {
    "@types/node": "^22.5.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.4",
    "jest": "^29.0.0",
    "@types/jest": "^29.0.0",
    "ts-jest": "^29.0.0",
    "eslint": "^8.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "@typescript-eslint/eslint-plugin": "^5.0.0"
  },
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=14.0.0",
    "npm": ">=7.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "collectCoverage": true,
    "coverageDirectory": "coverage"
  }
}
